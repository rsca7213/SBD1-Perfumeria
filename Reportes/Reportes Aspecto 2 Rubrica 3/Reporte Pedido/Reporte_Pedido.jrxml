	<subDataset name="ProductosDePedido" uuid="93baf9a7-78cd-4289-ab65-8cd76a04396e">
		<property name="com.jaspersoft.studio.data.defaultdataadapter" value="ProyectoBasesAdapter"/>
		<property name="com.jaspersoft.studio.data.sql.tables" value=""/>
		<parameter name="NumPedido" class="java.lang.Integer"/>
		<queryString language="plsql">
			<![CDATA[SELECT i.nombre as nombre,presentIng.volumen as present,i.nombre || ' (' || presentIng.volumen || ' ml )' AS presentacion, dp.cantidad AS cantidad, TRUNC(dp.precio/dp.cantidad,2) || ' $' AS precio,dp.precio || ' $' AS subtotal       
FROM rdj_pedidos p, rdj_detalles_pedidos dp, rdj_presents_ings_esencias presentIng, rdj_ingredientes_esencias i
WHERE dp.num_pedido=$P{NumPedido} AND dp.num_pedido=p.num_pedido AND i.cas_ing_esencia=dp.cas_esencia AND dp.id_pres_esencia=presentIng.id AND i.cas_ing_esencia=presentIng.cas_ing_esencia
UNION
SELECT o.nombre as nombre,presentOIng.volumen as present,o.nombre || ' (' || presentOIng.volumen || ' ml )' AS presentacion, dp.cantidad AS cantidad, TRUNC(dp.precio/dp.cantidad,2) || ' $' AS precio,dp.precio || ' $' AS subtotal        
FROM rdj_pedidos p, rdj_detalles_pedidos dp, rdj_present_otros_ings presentOIng, rdj_otros_ingredientes o
WHERE dp.num_pedido=$P{NumPedido} AND dp.num_pedido=p.num_pedido AND o.cas_otro_ing=dp.cas_otro AND dp.id_pres_otro=presentOIng.id AND o.cas_otro_ing=presentOIng.cas_otro_ing
ORDER BY nombre,present ASC;]]>
		</queryString>
		<field name="presentacion" class="java.lang.String"/>
		<field name="cantidad" class="java.lang.Integer"/>
		<field name="precio" class="java.lang.String"/>
		<field name="subtotal" class="java.lang.String"/>
	</subDataset>
	<subDataset name="Pagos" uuid="95c53d31-e550-41a3-9c4f-ff5dc95c7e52">
		<property name="com.jaspersoft.studio.data.defaultdataadapter" value="ProyectoBasesAdapter"/>
		<property name="com.jaspersoft.studio.data.sql.tables" value=""/>
		<parameter name="ParametroPago" class="java.lang.Integer"/>
		<queryString language="plsql">
			<![CDATA[SELECT to_char(fecha_pago,'dd/mm/yyyy') AS pagado_en, monto || ' $' AS monto_pagado 
FROM rdj_pagos 
WHERE num_pedido=$P{ParametroPago}]]>
		</queryString>
		<field name="pagado_en" class="java.lang.String"/>
		<field name="monto_pagado" class="java.lang.String"/>
	</subDataset>
	<parameter name="IdPedido" class="java.lang.Integer"/>
	<queryString language="plsql">
		<![CDATA[SELECT prod.nombre AS prod_nombre,prod.telefono AS prod_telefono,prod.correo AS prod_correo,prod.url_web AS prod_url,
prov.nombre AS prov_nombre,prov.telefono AS prov_telefono,prov.correo AS prov_correo,prov.url_web AS prov_url,
 y.pago AS pago_ped,x.duracion AS duracion_ped,x.precio || ' $' AS precio_ped,x.envio AS envio_ped, (ped.monto - x.precio)||' $' AS total_ing,
CASE WHEN ped.estatus='e' THEN 'Enviado' WHEN ped.estatus='p' THEN 'Pendiente'
WHEN ped.estatus='cprov' THEN 'Cancelado por Proveedor' WHEN ped.estatus='cprod' THEN 'Cancelado por Productor'
END AS estatus_pedido, CASE WHEN to_char(ped.fecha_envio,'dd/mm/yyyy') is NULL THEN 'N/A' ELSE to_char(ped.fecha_envio,'dd/mm/yyyy') END AS fecha_envio_pedido,
to_char(ped.fecha_pedido,'dd/mm/yyyy') AS fecha_pedido
FROM rdj_productores AS prod, rdj_proveedores AS prov, rdj_pedidos AS ped,
(SELECT CASE WHEN env.tipo ='t' THEN 'Terrestre' WHEN env.tipo='m'
THEN 'Marítimo' ELSE 'Aéreo' END || ' - ' || c.nombre AS envio, CASE WHEN env.duracion=1 THEN env.duracion||' mes' ELSE env.duracion || ' Meses' END AS duracion, env.precio AS precio 
FROM rdj_pedidos AS ped, rdj_metodos_contratos AS met,rdj_metodos_envios AS env, rdj_paises AS c
WHERE ped.num_pedido=$P{IdPedido} AND ped.id_envio=met.id AND met.id_envio=env.id AND c.id=env.id_pais) AS x,
(SELECT CASE WHEN pag.tipo ='c' THEN 'Completo' ELSE pag.num_cuotas || ' cuotas fijas de ' ||
pag.porcentaje || '% cada ' || CASE WHEN pag.meses=1 THEN pag.meses|| ' mes' ELSE pag.meses|| ' meses' END END AS pago 
FROM rdj_pedidos AS ped, rdj_metodos_contratos AS met,rdj_metodos_pagos AS pag
WHERE ped.num_pedido=$P{IdPedido} AND ped.id_pago=met.id AND met.id_pago=pag.id) AS y
WHERE ped.num_pedido=$P{IdPedido} AND prod.id=ped.id_productor AND prov.id=ped.id_proveedor;]]>
	</queryString>
