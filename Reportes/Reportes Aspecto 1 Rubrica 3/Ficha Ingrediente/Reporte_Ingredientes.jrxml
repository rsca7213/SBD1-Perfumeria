	<subDataset name="tabla_familias" uuid="6942918c-8ad5-4261-b789-314f3c43ed91">
		<parameter name="cas" class="java.lang.Integer"/>
		<queryString language="plsql">
			<![CDATA[SELECT f.nombre AS familia 
FROM rdj_ings_esencias_familias ifa, rdj_familias_olfativas f
WHERE ifa.id_familia=f.id AND ifa.cas_ing_esencia=$P{cas};]]>
		</queryString>
		<field name="familia" class="java.lang.String"/>
	</subDataset>
	<parameter name="cas" class="java.lang.Integer"/>
	<queryString language="plsql">
		<![CDATA[SELECT (to_char(i.cas_ing_esencia,'99999-99-9')) AS cas, i.nombre AS nombre, 
CASE WHEN i.num_einecs IS NULL THEN 'N/A' ELSE (to_char(i.num_einecs,'99999-99-9')) END AS einecs, p.nombre AS pais, 
CASE WHEN i.naturaleza='s' THEN 'Ingrediente Sintético' ELSE 'Ingrediente Natural' END AS naturaleza, 
CASE WHEN i.solubilidad IS NULL THEN 'N/A' ELSE (i.solubilidad || ' %') END AS solubilidad, (i.pto_inf || ' °C') AS punto_inflamacion,
CASE WHEN i.proc_creac IS NULL THEN 'Se desconoce su proceso de creación' ELSE (i.proc_creac) END AS creacion, 
CASE WHEN i.vida_util IS NULL THEN 'N/A' ELSE (i.vida_util || ' días') END AS vida_util 
FROM rdj_ingredientes_esencias i, rdj_paises p, rdj_ings_esencias_paises ip
WHERE i.cas_ing_esencia=ip.cas_ing_esencia AND ip.id_pais=p.id  AND i.cas_ing_esencia=$P{cas};]]>
	</queryString>
	<field name="nombre" class="java.lang.String"/>
